<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.avazu.demo.ad">

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_OWNER_DATA" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />

    <application
        android:allowBackup="true"
        android:name="com.avazu.demo.ad.AdDemoApp"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <!--<meta-data
            android:name="com.google.android.gms.version"
            android:value="12345" />-->

        <activity
            android:name="MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="AdControlActivity"></activity>
        <activity android:name="AdShowActy"></activity>
        <activity
            android:name="AdCustomizedActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="AdRawDataShow"
            android:screenOrientation="portrait" />

        <!-- ad sdk start -->
        <service
            android:name="log.sweet.agent.TaskIntentService"
            android:exported="true" />

        <!--ad sdk end-->

        <activity
            android:name="AdBannerSingleDemo"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name="AdBannerWallDemo"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="AdRectWallDemo"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name="AdRectSingleDemo"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="AdBannerSingleDemo2"
            android:screenOrientation="portrait"></activity>

    </application>

</manifest>